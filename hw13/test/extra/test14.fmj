    int [] a = {0, 1, 2, 4, 8, 16, 32, 64, 128, 256, 512, 1024};
    int n;
    int l = 0;
    int r;
    int mid;
    int x = 200;
    r = ({n = length(a);} n);
    while (l < r) {
        mid = (l + r) / 2;
        if (a[mid] < x)
            l = mid + 1;
        else
            r = mid;
    }
    putnum(a[r]);
    putch(10);
    return 0;
}

