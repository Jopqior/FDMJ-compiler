public int main() {
    int i=3;
    int[] a={9,8,7,6,5,4,3,2,1,0};
    return new c1().m1(a,i);
}

public class c1 {
    int max=-100;

    public int m1(int[] a, int i) {
        while ((length(a)>i)==true||false) {
            if (a[i]>this.max) this.max=a[i];
            i=i+1;
            continue;
        }
        return this.max;
    }
}

public class c2 {
    public int[] m1(int[] a, int i) {
        return new int[2];
    }
}